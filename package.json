  "scripts": {
    "zelfrontdev": "webpack-dev-server --host 0.0.0.0 --port 16126 --inline --progress --config ZelFront/build/webpack.dev.conf.js",
    "zelfrontprod": "webpack-dev-server --host 0.0.0.0 --port 16126 --inline --progress --config ZelFront/build/webpack.prod.conf.js",
    "zelfrontstart": "npm run zelfrontprod",
    "zelfrontunit": "cross-env BABEL_ENV=test karma start ZelFront/test/unit/karma.conf.js --single-run",
    "zelfronte2e": "node ZelFront/test/e2e/runner.js",
    "zelfronttest": "npm run unit && npm run e2e",
    "zelfrontlint": "eslint --ext .js,.vue src ZelFront/test/unit ZelFront/test/e2e/specs",
    "zelfrontbuild": "node ZelFront/build/build.js",
    "zelbackstart": "nodemon ZelBack/app.js",
    "start": "node init.js && (npm run zelbackstart >> zelback.debug & npm run zelfrontstart &>&1 | tee zelfront.debug)",
    "dev": "node init.js && (npm run zelbackstart & npm run zelfrontdev)"
  },
